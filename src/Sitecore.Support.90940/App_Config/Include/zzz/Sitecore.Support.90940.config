    <pipelines>
      <initialize>
        <processor type="Sitecore.Pipelines.Loader.ShowVersion, Sitecore.Kernel">
          <assemblies>
            <assembly id="Sitecore.Support.90940">/bin/Sitecore.Support.90940.dll</assembly>
          </assemblies>
        </processor>
      </initialize>
      <httpRequestBegin>
        <processor type="Sitecore.Support.Pipelines.HttpRequest.ItemResolver, Sitecore.Support.90940" patch:instead="*[@type='Sitecore.Pipelines.HttpRequest.ItemResolver, Sitecore.Kernel']" />
      </httpRequestBegin>
    </pipelines>
  </sitecore>
